[run]
source = backend, services
omit = 
    */site-packages/*
    */venv/*
    */env/*
    */__pycache__/*
    */migrations/*
    */tests/*
    */test_*
    */conftest.py
    .venv/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod

[html]
directory = htmlcov
