# Scorpius X - Environment Configuration
# Copy this file to .env and configure your local values

# Application Configuration
NODE_ENV=development
VITE_APP_NAME="Scorpius X"
VITE_APP_VERSION="1.0.0"

# API Configuration
VITE_API_BASE_URL=http://localhost:8000
VITE_API_TIMEOUT=30000

# Security Configuration
VITE_ENABLE_SECURITY_HEADERS=true
VITE_CSRF_PROTECTION=true

# Blockchain Configuration
VITE_ETH_MAINNET_RPC=
VITE_ETH_GOERLI_RPC=
VITE_POLYGON_RPC=
VITE_BSC_RPC=

# Analytics Configuration
VITE_ANALYTICS_ENABLED=false
VITE_ANALYTICS_TRACKING_ID=

# Feature Flags
VITE_ENABLE_MEV_OPERATIONS=true
VITE_ENABLE_QUANTUM_SECURITY=false
VITE_ENABLE_AI_TRADING=true

# External Services
VITE_ETHERSCAN_API_KEY=
VITE_COINGECKO_API_KEY=
VITE_DEFIPULSE_API_KEY=

# Development Configuration
VITE_DEBUG_MODE=false
VITE_MOCK_DATA=false
