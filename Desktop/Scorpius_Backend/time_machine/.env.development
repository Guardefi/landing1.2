# Time Machine - Development Environment
APP_ENV=development
DEBUG=true
HOST=0.0.0.0
PORT=8000

# Development Database
DATABASE_URL=sqlite:///./dev_time_machine.db

# Development RPC (use free tier)
ETH_RPC_URL=https://eth-mainnet.g.alchemy.com/v2/demo
ETH_WS_URL=wss://eth-mainnet.g.alchemy.com/v2/demo

# Development Keys (insecure - for development only)
SECRET_KEY=dev-secret-key-do-not-use-in-production
JWT_SECRET_KEY=dev-jwt-secret-do-not-use-in-production

# Development CORS
CORS_ORIGINS=["http://localhost:3000", "http://localhost:3001", "http://127.0.0.1:3000", "http://127.0.0.1:3001"]

# Development Logging
LOG_LEVEL=DEBUG
LOG_FORMAT=pretty

# Development Features
ENABLE_DEMO_DATA=true
ENABLE_REAL_TIME_MONITORING=false

# Development Rate Limiting (disabled)
RATE_LIMIT_PER_MINUTE=1000
