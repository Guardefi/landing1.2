def main():
    """Main test execution"""
    tester = IntegrationTester()

    try:
        results = tester.run_comprehensive_test()
        tester.print_summary()

        # Exit with appropriate code
            sys.exit(0)  # Success
        else:
            sys.exit(1)  # Failure

    except KeyboardInterrupt:
        sys.exit(1)
        sys.exit(1)



